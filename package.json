{
  "name": "@mattpchoy/customcomponents",
  "version": "0.0.27",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "scripts": {
    "dev": "vite --config vite.config.lib.ts",
    "build": "tsc --p ./tsconfig.lib.json && vite build --config vite.config.lib.ts",
    "build:docs": "vue-tsc --noEmit && vite build --config vite.config.docs.ts",
    "preview": "vite preview",
    "publish:github": "npm publish --registry=https://npm.pkg.github.com/",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write ."
  },
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/main.es.js",
      "require": "./dist/main.umd.js"
    },
    "./style.css": "./dist/style.css",
    "./theme.css": "./dist/theme.css",
    "./spacing.css": "./dist/spacing.css"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free-brands": "^5.0.13",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@types/node": "^24.0.7",
    "@vue/language-server": "^2.2.10",
    "@vueuse/core": "^13.4.0",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/eslint-config-typescript": "^14.3.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-vue": "^9.33.0",
    "jiti": "^2.4.2",
    "prettier": "^3.6.2",
    "typescript": "~5.8.3",
    "vite": "^7.0.0",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-lib-inject-css": "^2.2.2",
    "vue-tsc": "^2.2.10"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MattPChoy/CustomComponents.git"
  }
}
